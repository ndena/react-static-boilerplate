{
  "name": "nd-web-static",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently --raw \"yarn run server\" \"yarn run build:watch\"",
    "clean": "rm -rf dist",
    "build": "cp .env.dev .env && yarn run clean && node -r babel $(yarn bin)/webpack --env.dev",
    "build:watch": "cp .env.dev .env && node -r @babel/register node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --env.dev",
    "build:qa": "cp .env.qa .env && yarn run clean && yarn run optimize && node -r babel $(yarn bin)/webpack && yarn run upload",
    "build:test": "cp .env.qa .env && yarn run clean && node -r babel $(yarn bin)/webpack",
    "server": "NODE_PATH=./src node -r @babel/register bin/server",
    "upload": "NODE_PATH=./src node -r babel src/cdn/assetuploader",
    "optimize": "NODE_PATH=./src node -r babel src/cdn/imageOptimizer",
    "lint": "eslint src"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.17.1",
    "babel-runtime": "^6.23.0",
    "chai": "^4.2.0",
    "classnames": "^2.2.5",
    "commander": "^2.9.0",
    "concurrently": "^3.3.0",
    "domready": "^1.0.8",
    "dotenv": "^4.0.0",
    "eslint": "^5.11.1",
    "handlebars": "^4.0.14",
    "history": "^4.7.2",
    "koa": "next",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-proxy": "^0.8.0",
    "koa-static": "^2.1.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "15.5.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^6.0.0",
    "react-themed": "3.2.1",
    "recompose": "^0.25.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.2.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
